generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://myuser:mypassword@db:5432/ecommerce?schema=public"
}

model User {
  id                   String                 @id @default(uuid())
  name                 String
  email                String                 @unique
  password             String
  cpf                  String
  role                 String
  created_at           DateTime               @default(now())
  google_status        Boolean
  google_id            String?

  @@map("users")
}
